{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "plugins": [
        "prettier",
        "unused-imports"
    ],
    "extends": [
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "unused-imports/no-unused-imports": 2,
        "unused-imports/no-unused-vars": 1
    }
}
